{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "private": false,
  "type": "module",
  "files": ["dist"],
  "main": "./dist/<%= name %>.umd.js",
  "module": "./dist/<%= name %>.es.js",
  "exports": {
      ".": {
          "import": "./dist/<%= name %>.es.js",
          "require": "./dist/<%= name %>.umd.js"
      }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "license": "ISC",
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2",
    "less": "^4.1.3",
    "less-loader": "^11.1.2"
  }
}
